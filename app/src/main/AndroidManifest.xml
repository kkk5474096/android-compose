<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.backpac.composeandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeAndroid"
        tools:targetApi="31">
        <activity
            android:name=".layout.LayoutActivity"
            android:exported="true"
            android:theme="@style/Theme.ComposeAndroid">

        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeAndroid" />
        <activity
            android:name=".guide.GuideActivity"
            android:exported="true" />

        <activity
            android:name=".list.ListActivity"
            android:exported="true" />

        <activity android:name=".stagger.StaggerActivity"
            android:exported="true" />

        <activity android:name=".slide.SlideActivity"
            android:exported="true" >

        </activity>

        <activity android:name=".slot.SlotActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>